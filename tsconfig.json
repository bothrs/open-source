{
  "compilerOptions": {
    "target": "es5",
    "lib": ["esnext"],
    "allowJs": false,
    "checkJs": false,
    "importHelpers": false,
    "declaration": true,
    "removeComments": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noImplicitAny": true,
  }
}
