{
  "name": "@bothrs/util",
  "version": "3.2.0",
  "description": "Common helper functions",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run clean && tsc -w --noEmit",
    "clean": "find . -maxdepth 1 -type f \\( -name \\*.mjs -or -name \\*.js -or -name \\*.d.ts -or -name \\*.jsx \\) -delete; date",
    "build:deprecated": "npm run clean && tsc -d && cp src/mjs/*.mjs .",
    "build": "yarn build:clean && parcel build && mkdir dist/mjs && cp src/mjs/*.mjs dist/mjs",
    "build:clean": "rimraf \"./dist\"",
    "docs": "typedoc --out docs src --includeVersion",
    "test": "jest --config=\"../../jest.config.js\" --roots=\"./packages/util/src\" --coverage --runInBand",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "~27.5.2",
    "@types/node-fetch": "2",
    "documentation": "^13.2.5",
    "node-fetch": "^2.6.1",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-node": "^10.8.0",
    "tslib": "^2.3.1",
    "typedoc": "^0.21.4",
    "typescript": "~4.6.4"
  },
  "dependencies": {
    "@bothrs/math": "^0.1.5"
  },
  "peerDependencies": {
    "node-fetch": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:bothrs/open-source.git"
  },
  "contributors": [
    "Thomas Ghysels <info@thomasg.be> (https://thomasg.be)",
    "Jacco Goris <contact@jaccogoris.com> (https://jaccogoris.com)",
    "Fabian Meul <fabian@bothrs.com>"
  ]
}
